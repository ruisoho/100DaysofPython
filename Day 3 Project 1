print("The Love Calculator is calculating your score...")
name1 = input() # What is your name?
name2 = input() # What is their name?
# ðŸš¨ Don't change the code above ðŸ‘†
# Write your code below this line ðŸ‘‡
#Combining the names together as one String
combined_names = name1 + name2
#Set all the letters to lower case
lower_names = combined_names.lower()
#Create a variable to all letter from word "True" and count it
t = lower_names.count("t")
r = lower_names.count("r")
u = lower_names.count("u")
e = lower_names.count("e")
#create a variable to sume all letters in the first word
first_word = t + r + u + e
#Create a variable to all letter from word "Love" and count it
l = lower_names.count("l")
o = lower_names.count("o")
v = lower_names.count("v")
e = lower_names.count("e")
secound_word = l + o + v + e
#Calculate both Strings together and transforme it as an Interger
score = int(str(first_word)+ str(secound_word))
#If Statement to put out the score results.
if (score < 10) or(score > 90):
    print(f"Your score is {score}, you go together like coke and mentos.")
elif (score >= 40) and (score <= 50):
  print(f"Your score is {score}, you are alright together.")
else:
  print(f"Your score is {score}.")
